  {
    "name": "Tab Stack",
    "version": "1.0",
    "description": "developing..",

    "manifest_version": 2,

   	"permissions": ["activeTab", "tabs","storage"],


    "background": {
      "scripts": ["background.js"]
    },

    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "128": "images/tab_stack_128.png"
      }
    },

    "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Alt+2"
        }
      }
    }

    
  }